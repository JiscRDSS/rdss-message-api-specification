{
  "id": "https://www.jisc.ac.uk/rdss/schema/thesis_dissertation.json/#",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "ThesisDissertation": {
      "type": "object",
      "properties": {
        "objectUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "objectTitle": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "objectPersonRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/PersonRole"
          },
          "minItems": 1
        },
        "objectDescription": {
          "type": "array", 
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Description"
          }
        },
        "objectRights": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Rights"
        },
        "objectDate": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Date"
          },
          "minItems": 1
        },
        "objectKeywords": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "objectCategory": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "objectResourceType": {
          "type": "string",
          "enum": ["thesisDissertation"]
        },
        "objectValue": {
          "type": "integer",
          "minimum": 1,
          "maximum": 3
        },
        "objectIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Identifier"
          },
          "minItems": 1
        },
        "objectRelatedIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/IdentifierRelationship"
          }
        },
        "objectOrganisationRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/OrganisationRole"
          },
          "minItems": 1
        },
        "objectPreservationEvent": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/PreservationEvent"
          }
        },
        "objectFile": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/File"
          }
        },
        "coverage": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/common_entities.json/#/definitions/Coverage"
        },
        "language": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        }, 
        "qualificationLevel": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "qualificationName": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "yearOfAward": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "thesisDissertationType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 2
        }
      },
      "required": [
        "objectUUID",
        "objectTitle",
        "objectPersonRole",
        "objectDescription",
        "objectRights",
        "objectDate",
        "objectResourceType",
        "objectValue",
        "objectIdentifier",
        "objectOrganisationRole",
        "language",
        "qualificationLevel",
        "qualificationName",
        "yearOfAward",
        "thesisDissertationType"
      ],
      "additionalProperties": false
    }
  }
}
