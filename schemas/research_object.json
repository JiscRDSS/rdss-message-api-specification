{
  "id": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "dataset": {
      "type": "object",
      "properties": {
        "datasetUuid": {
          "type": "string"
        },
        "datasetTitle": {
          "type": "string"
        },
        "datasetSubject": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "datasetDescription": {
          "type": "string"
        },
        "datasetKeywords": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "datasetRights": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/rights"
          },
          "minItems": 1
        },
        "datasetDate": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/date"
          },
          "minItems": 1
        },
        "datasetLanguage": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "datasetCoverage": {
          "$ref": "#/definitions/coverage"
        },
        "datasetResourceType": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "minItems": 1
        },
        "datasetVersion": {
          "type": "string"
        },
        "datasetValue": {
          "type": "number"
        },
        "datasetIdentifier": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/objectIdentifier"
          },
          "minItems": 1
        },
        "datasetRelatedIdentifier": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/objectIdentifier"
          }
        },
        "datasetCitedBy": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/publication"
          }
        },
        "datasetOrganisation": {
          "type": "array",
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/organisation"
          },
          "minItems": 1
        },
        "datasetFile": {
          "type": "array",
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/file"
          }
        },
        "datasetAdditionalMetadata": {
          "type": "string"
        },
        "datasetPerson": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/datasetPerson"
          },
          "minItems": 1
        }
      },
      "required": [
        "datasetUuid",
        "datasetTitle",
        "datasetDescription",
        "datasetKeywords",
        "datasetRights",
        "datasetDate",
        "datasetResourceType",
        "datasetValue",
        "datasetIdentifier",
        "datasetOrganisation",
        "datasetPerson"
      ]
    },
    "datasetPerson": {
      "type": "object",
      "properties": {
        "datasetPerson": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/person"
        },
        "datasetPersonRole": {
          "type": "number"
        }
      },
      "required": [
        "datasetPerson",
        "datasetPersonRole"
      ]
    },
    "date": {
      "type": "object",
      "properties": {
        "dateValue": {
          "type": "string"
        },
        "dateType": {
          "type": "number"
        }
      },
      "required": [
        "dateValue",
        "dateType"
      ]
    },
    "rights": {
      "type": "object",
      "properties": {
        "rightsStatement": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "rightsHolder": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "licence": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/licence"
          },
          "minItems": 1
        },
        "access": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/access"
          },
          "minItems": 1
        }
      },
      "required": [
        "rightsStatement",
        "rightsHolder",
        "licence",
        "access"
      ]
    },
    "licence": {
      "type": "object",
      "properties": {
        "licenceName": {
          "type": "string"
        },
        "licenceIdentifier": {
          "type": "string"
        }
      },
      "required": [
        "licenceName",
        "licenceIdentifier"
      ]
    },
    "access": {
      "type": "object",
      "properties": {
        "accessType": {
          "type": "number"
        },
        "accessStatement": {
          "type": "string"
        }
      },
      "required": [
        "accessType"
      ]
    },
    "coverage": {
      "type": "object",
      "properties": {
        "geospatialCoverage": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/geospatialCoverage"
          }
        },
        "temporalCoverageStart": {
          "$ref": "#/definitions/date"
        },
        "temporalCoverageEnd": {
          "$ref": "#/definitions/date"
        }
      }
    },
    "geospatialCoverage": {
      "type": "object",
      "properties": {
        "geolocationPoint": {
          "$ref": "#/definitions/geolocationPoint"
        },
        "geolocationPolygon": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/geolocationPoint"
          }
        },
        "geolocationPlace": {
          "type": "string"
        },
        "coordinateReferenceSystem": {
          "type": "string"
        }
      },
      "required": [
        "coordinateReferenceSystem"
      ]
    },
    "geolocationPoint": {
      "type": "object",
      "properties": {
        "x": {
          "type": "number"
        },
        "y": {
          "type": "number"
        }
      },
      "required": [
        "x",
        "y"
      ]
    },
    "objectIdentifier": {
      "type": "object",
      "properties": {
        "objectIdentifierValue": {
          "type": "string"
        },
        "objectIdentifierType": {
          "type": "number"
        },
        "relationType": {
          "type": "number"
        }
      },
      "required": [
        "objectIdentifierValue",
        "objectIdentifierType"
      ]
    },
    "collectiveObject": {
      "type": "object",
      "properties": {
        "collectiveObjectUuid": {
          "type": "string"
        },
        "collectiveObjectName": {
          "type": "string"
        },
        "collectiveObjectIdentifier": {
          "$ref": "#/definitions/objectIdentifier"
        },
        "collectiveObjectDataset": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/dataset"
          }
        },
        "collectiveObjectPublication": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/publication"
          }
        },
        "collectiveObjectSoftware": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/software"
          }
        }
      },
      "required": [
        "collectiveObjectUuid",
        "collectiveObjectName",
        "collectiveObjectIdentifier"
      ]
    },
    "publication": {
      "type": "object",
      "properties": {
        "publicationUuid": {
          "type": "string"
        },
        "publicationFreeToRead": {
          "type": "boolean"
        },
        "publicationIdentifier": {
          "$ref": "#/definitions/objectIdentifier"
        },
        "publicationCoverage": {
          "$ref": "#/definitions/coverage"
        },
        "publicationDescription": {
          "type": "string"
        },
        "publicationFile": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/file"
        },
        "publicationLanguage": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "publicationResourceType": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "minItems": 1
        },
        "publicationTitle": {
          "type": "string"
        },
        "publicationSubject": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "publicationDate": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/date"
          },
          "minItems": 1
        },
        "publicationApc": {
          "type": "number"
        },
        "publicationRelatedIdentifier": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/objectIdentifier"
          }
        },
        "publicationVersion": {
          "type": "number"
        },
        "publicationRights": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/rights"
          },
          "minItems": 1
        },
        "publicationPerson": {
          "type": "array",
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/person"
          },
          "minItems": 1
        },
        "publicationOrganisation": {
          "type": "array",
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/organisation"
          },
          "minItems": 1
        },
        "publicationProject": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/project"
        },
        "publicationAdditionalMetadata": {
          "type": "string"
        }
      },
      "required": [
        "publicationUuid",
        "publicationFreeToRead",
        "publicationIdentifier",
        "publicationDescription",
        "publicationFile",
        "publicationLanguage",
        "publicationResourceType",
        "publicationTitle",
        "publicationDate",
        "publicationVersion",
        "publicationRights",
        "publicationPerson",
        "publicationOrganisation"
      ]
    },
    "software": {
      "type": "object",
      "properties": {
        "softwareUuid": {
          "type": "string"
        },
        "softwareName": {
          "type": "string"
        },
        "softwareIdentifier": {
          "$ref": "#/definitions/objectIdentifier"
        },
        "softwareDescription": {
          "type": "string"
        },
        "softwareVersion": {
          "type": "string"
        },
        "softwareRights": {
          "$ref": "#/definitions/rights"
        },
        "softwareLanguage": {
          "type": "string"
        },
        "softwareVendor": {
          "type": "string"
        },
        "softwareDatasetLinkage": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": [
        "softwareUuid",
        "softwareName",
        "softwareIdentifier",
        "softwareDescription",
        "softwareVersion",
        "softwareRights"
      ]
    }
  }
}
