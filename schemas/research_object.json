{
  "id": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "object": {
      "type": "object",
      "properties": {
        "objectUuid": {
          "type": "string"
        },
        "objectTitle": {
          "type": "string"
        },
        "objectContributor": {
          "type": "array",
          "items": {
            "type": "number"
          },
          "minItems": 1
        },
        "objectDescription": {
          "type": "string"
        },
        "objectRights": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/rights"
          },
          "minItems": 1
        },
        "objectDate": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/date"
          },
          "minItems": 1
        },
        "objectResourceType": {
          "type": "object"
        },
        "objectValue": {
          "type": "number"
        },
        "objectIdentifier": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/objectIdentifier"
          },
          "minItems": 1
        },
        "objectRelatedIdentifier": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/objectIdentifier"
          }
        },
        "objectPublisher": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/organisationRole"
          },
          "minItems": 1
        },
        "objectFile": {
          "type": "array",
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/file"
          }
        }
      },
      "required": [
        "objectUuid",
        "objectTitle",
        "objectContributor",
        "objectDescription",
        "objectRights",
        "objectDate",
        "objectResourceType",
        "objectValue",
        "objectIdentifier",
        "objectPublisher"
      ]
    },
    "objectIdentifier": {
      "type": "object",
      "properties": {
        "objectIdentifierValue": {
          "type": "string"
        },
        "objectIdentifierType": {
          "type": "number"
        },
        "relationType": {
          "type": "number"
        }
      },
      "required": [
        "objectIdentifierValue",
        "objectIdentifierType",
        "relationType"
      ]
    },
    "rights": {
      "type": "object",
      "properties": {
        "rightsStatement": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "rightsHolder": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "licence": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/licence"
          },
          "minItems": 1
        },
        "access": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/access"
          },
          "minItems": 1
        }
      },
      "required": [
        "rightsStatement",
        "rightsHolder",
        "licence",
        "access"
      ]
    },
    "licence": {
      "type": "object",
      "properties": {
        "licenceName": {
          "type": "string"
        },
        "licenceIdentifier": {
          "type": "string"
        }
      },
      "required": [
        "licenceName",
        "licenceIdentifier"
      ]
    },
    "access": {
      "type": "object",
      "properties": {
        "accessType": {
          "type": "number"
        },
        "accessStatement": {
          "type": "string"
        }
      },
      "required": [
        "accessType"
      ]
    },
    "collection": {
      "type": "object",
      "properties": {
        "collectionUuid": {
          "type": "string"
        },
        "collectionName": {
          "type": "string"
        },
        "collection": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/object"
          }
        },
        "collectionDescription": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "minItems": 1
        },
        "collectionRights": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/rights"
          },
          "minItems": 1
        }
      },
      "required": [
        "collectionUuid",
        "collectionName",
        "collectionDescription",
        "collectionRights"
      ]
    },
    "organisationRole": {
      "type": "object",
      "properties": {
        "organisation": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/organisation"
        },
        "role": {
          "type": "number"
        }
      },
      "required": [
        "organisation",
        "role"
      ]
    },
    "personRole": {
      "type": "object",
      "properties": {
        "person": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/person"
        },
        "role": {
          "type": "number"
        }
      },
      "required": [
        "person",
        "role"
      ]
    },
    "date": {
      "type": "object",
      "properties": {
        "dateValue": {
          "type": "string"
        },
        "dateType": {
          "type": "number"
        }
      },
      "required": [
        "dateValue",
        "dateType"
      ]
    }
  }
}
