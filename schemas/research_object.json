{
  "id": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "ResearchObject": {
      "type": "object",
      "properties": {
        "objectUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "objectTitle": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "objectPersonRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/PersonRole"
          },
          "minItems": 1
        },
        "objectDescription": {
          "type": "array", 
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Description"
          }
        },
        "objectRights": {
          "$ref": "#/definitions/Rights"
        },
        "objectDate": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Date"
          },
          "minItems": 1
        },
        "objectKeywords": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "objectCategory": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "objectResourceType": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/ResourceType"
        },
        "objectValue": {
          "type": "integer",
          "minimum": 1,
          "maximum": 3
        },
        "objectIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Identifier"
          },
          "minItems": 1
        },
        "objectRelatedIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/IdentifierRelationship"
          }
        },
        "objectOrganisationRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/OrganisationRole"
          },
          "minItems": 1
        },
        "objectPreservationEvent": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/PreservationEvent"
          }
        },
        "objectFile": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#/definitions/File"
          }
        }
      },
      "required": [
        "objectUUID",
        "objectTitle",
        "objectPersonRole",
        "objectDescription",
        "objectRights",
        "objectDate",
        "objectResourceType",
        "objectIdentifier",
        "objectOrganisationRole"
      ],
      "additionalProperties": false
    },
    "IdentifierRelationship": {
      "type": "object",
      "properties": {
        "identifier": {
          "$ref": "#/definitions/Identifier"
        },
        "relationType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 37
        }
      },
      "required": [
        "identifier",
        "relationType"
      ],
      "additionalProperties": false
    },
    "Identifier": {
      "type": "object",
      "properties": {
        "identifierValue": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "identifierType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 19
        }
      },
      "required": [
        "identifierValue",
        "identifierType"
      ],
      "additionalProperties": false
    },
    "Rights": {
      "type": "object",
      "properties": {
        "rightsStatement": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "rightsHolder": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "licence": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Licence"
          },
          "minItems": 1
        },
        "access": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Access"
          },
          "minItems": 1
        }
      },
      "required": [
        "licence",
        "access"
      ],
      "additionalProperties": false
    },
    "Licence": {
      "type": "object",
      "properties": {
        "licenceName": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "licenceIdentifier": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "licenseStartDate": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "licenseEndDate": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        }
      },
      "required": [
        "licenceIdentifier"
      ],
      "additionalProperties": false
    },
    "Access": {
      "type": "object",
      "properties": {
        "accessType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 5
        },
        "accessStatement": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        }
      },
      "required": [
        "accessType"
      ],
      "additionalProperties": false
    },
    "Collection": {
      "type": "object",
      "properties": {
        "collectionUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "collectionName": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "collectionObject": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Object"
          }
        },
        "collectionKeywords": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "collectionCategory": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "collectionDescription": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          },
          "minItems": 1
        },
        "collectionRights": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Rights"
          },
          "minItems": 1
        },
        "collectionIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Identifier"
          }
        },
        "collectionRelatedIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/IdentifierRelationship"
          }
        },
        "collectionPersonRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/PersonRole"
          }
        },
        "collectionOrganisationRole": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/OrganisationRole"
          }
        }
      },
      "required": [
        "collectionUUID",
        "collectionName",
        "collectionDescription",
        "collectionRights"
      ],
      "additionalProperties": false
    },
    "OrganisationRole": {
      "type": "object",
      "properties": {
        "organisation": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/Organisation"
        },
        "role": {
          "type": "integer",
          "minimum": 1,
          "maximum": 10
        }
      },
      "required": [
        "organisation",
        "role"
      ],
      "additionalProperties": false
    },
    "PersonRole": {
      "type": "object",
      "properties": {
        "person": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/material_asset.json/#/definitions/Person"
        },
        "role": {
          "type": "integer",
          "minimum": 1,
          "maximum": 22
        }
      },
      "required": [
        "person",
        "role"
      ],
      "additionalProperties": false
    },
    "Date": {
      "type": "object",
      "properties": {
        "dateValue": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "dateType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 10
        }
      },
      "required": [
        "dateValue",
        "dateType"
      ],
      "additionalProperties": false
    },
    "Description": {
      "type": "object",
      "properties": {
        "descriptionValue": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "descriptionType": {
          "type": "integer",
          "minimum": 1,
          "maximum": 7
        }
      },
      "required": [
        "descriptionValue",
        "descriptionType"
      ],
      "additionalProperties": false
    }
  }
}
