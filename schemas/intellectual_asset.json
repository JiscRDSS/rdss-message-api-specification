{
  "id": "https://www.jisc.ac.uk/rdss/schema/intellectual_asset.json/#",
  "$schema": "http://json-schema.org/draft-06/schema#",
  "definitions": {
    "File": {
      "type": "object",
      "properties": {
        "fileUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "fileIdentifier": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileName": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileSize": {
          "type": "integer",
          "minimum": 0
        },
        "fileLabel": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileDateCreated": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "fileRights": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Rights"
        },
        "fileChecksum": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Checksum"
          },
          "minItems": 1
        },
        "fileFormatType": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileCompositionLevel": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileHasMimeType": {
          "type": "boolean"
        },
        "fileDateModified": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
          },
          "minItems": 1
        },
        "filePuid": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "fileUse": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/FileUse"
        },
        "filePreservationEvent": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "fileUploadStatus": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/UploadStatus"
        },
        "fileStorageStatus": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/StorageStatus"
        },
        "fileLastDownloaded": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "fileTechnicalAttribute": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
          }
        },
        "fileStorageLocation": {
          "type": "string",
          "format": "uri"
        },
        "fileStoragePlatform": {
          "$ref": "#/definitions/StoragePlatform"
        }
      },
      "required": [
        "fileUUID",
        "fileIdentifier",
        "fileName",
        "fileSize",
        "fileChecksum",
        "fileCompositionLevel",
        "fileDateModified",
        "fileUse",
        "fileUploadStatus",
        "fileStorageStatus",
        "fileStorageLocation",
        "fileStoragePlatform"
      ],
      "additionalProperties": false
    },
    "StoragePlatform": {
      "type": "object",
      "properties": {
        "storagePlatformUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "storagePlatformName": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "storagePlatformType": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/StorageType"
        },
        "storagePlatformCost": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        }
      },
      "required": [
        "storagePlatformUUID",
        "storagePlatformName",
        "storagePlatformType",
        "storagePlatformCost"
      ],
      "additionalProperties": false
    },
    "Checksum": {
      "type": "object",
      "properties": {
        "checksumUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "checksumType": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/enumeration.json/#/definitions/ChecksumType"
        },
        "checksumValue": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        }
      },
      "required": [
        "checksumUUID",
        "checksumType",
        "checksumValue"
      ],
      "additionalProperties": false
    },
    "Grant": {
      "type": "object",
      "properties": {
        "grantUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "grantIdentifier": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "grantFunder": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/OrganisationRole"
        },
        "grantStart": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "grantEnd": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "grantValue": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        }
      },
      "required": [
        "grantUUID",
        "grantIdentifier",
        "grantFunder",
        "grantStart",
        "grantEnd"
      ],
      "additionalProperties": false
    },
    "Project": {
      "type": "object",
      "properties": {
        "projectUUID": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/UUID"
        },
        "projectIdentifier": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Identifier"
          },
          "minItems": 1
        },
        "projectName": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "projectDescription": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/NonEmptyString"
        },
        "projectCollection": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "https://www.jisc.ac.uk/rdss/schema/research_object.json/#/definitions/Collection"
          },
          "minItems": 1
        },
        "projectGrant": {
          "type": "array",
          "uniqueItems": true,
          "items": {
            "$ref": "#/definitions/Grant"
          }
        },
        "projectStart": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        },
        "projectEnd": {
          "$ref": "https://www.jisc.ac.uk/rdss/schema/types.json/#/definitions/Timestamp"
        }
      },
      "required": [
        "projectUUID",
        "projectIdentifier",
        "projectName",
        "projectDescription",
        "projectCollection",
        "projectStart",
        "projectEnd"
      ],
      "additionalProperties": false
    }
  }
}
