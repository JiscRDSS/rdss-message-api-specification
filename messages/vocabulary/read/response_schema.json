  "$schema": "http://json-schema.org/draft-04/schema#",
  "vocabularyId": {
    "type": "number"
  },
  "terms": {
    "type": "array",
    "items": {
      "type": "object",
      "id": {
        "type": "number"
      },
      "name": {
        "type": "string"
      },
      "required": [
        "id",
        "name"
      ]
    }
  },
  "required": [
    "vocabularyId",
    "terms"
  ]
}
